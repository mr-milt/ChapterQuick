{
    "manifest_version": 3,
    "name": "ChapterQuick",
    "version": "1.1",
    "description": "Navigate to the next chapter on manga-scans.com and asuratoon.com using the Enter key.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "js/background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.manga-scans.com/*", "*://*.asuratoon.com/*", "*://*.webtoons.com/*", "*://*.reaperscans.com/*"],
        "js": ["js/content.js"]
      }
    ],
    "action": {
      "default_popup": "html/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  